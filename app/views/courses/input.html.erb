<h1>Course Input</h1>
<br>
<div class="float-right" id="upload">
  <button type="button" class="btn btn-info" id="upload">
    <i class="fas fa-upload"></i>
    Import Existing Plan
  </button>
</div>


<div class="input-group mb-3">
  <div class="input-group-prepend">
    <label class="input-group-text" for="inputGroupSelect01">Year</label>
  </div>
  <select class="custom-select" id="inputGroupSelect01">
    <option selected>Choose...</option>
    <option value="<%= (Date.today - 5.years).year %>"><%= (Date.today - 5.years).year %></option>
    <option value="<%= (Date.today - 4.years).year %>"><%= (Date.today - 4.years).year %></option>
    <option value="<%= (Date.today - 3.years).year %>"><%= (Date.today - 3.years).year %></option>
    <option value="<%= (Date.today - 2.years).year %>"><%= (Date.today - 2.years).year %></option>
    <option value="<%= (Date.today - 1.years).year %>"><%= (Date.today - 1.years).year %></option>
    <option value="<%= Date.today.year %>"><%= Date.today.year %></option>
    <option value="<%= (Date.today + 1.years).year %>"><%= (Date.today + 1.years).year %></option>
    <option value="<%= (Date.today + 2.years).year %>"><%= (Date.today + 2.years).year %></option>
    <option value="<%= (Date.today + 3.years).year %>"><%= (Date.today + 3.years).year %></option>
    <option value="<%= (Date.today + 4.years).year %>"><%= (Date.today + 4.years).year %></option>
    <option value="<%= (Date.today + 5.years).year %>"><%= (Date.today + 5.years).year %></option>
    <option value="<%= (Date.today + 6.years).year %>"><%= (Date.today + 6.years).year %></option>
  </select>
</div>

<div class="input-group mb-3">
  <div class="input-group-prepend">
    <label class="input-group-text" for="inputGroupSelect01">Term</label>
  </div>
  <select class="custom-select" id="inputGroupSelect01">
    <option selected>Choose...</option>
    <option value="Fa">Fall</option>
    <option value="Wi">Winter</option>
    <option value="Sp">Spring</option>
    <option value="Su">Summer</option>
  </select>
</div>

<div class="input-group mb-3">
  <div class="input-group-prepend">
    <span class="input-group-text" id="inputGroup-sizing-default">Course 1</span>
  </div>
  <input type="text" class="form-control" aria-label="Course" aria-describedby="inputGroup-sizing-default">
</div>

